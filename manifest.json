{
  "manifest_version": 3,
  "name": "Google Dorking Tool",
  "version": "1.0",
  "description": "A Chrome extension for performing Google Dorking searches.",
  "permissions": ["tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
